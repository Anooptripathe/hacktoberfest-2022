#include<bits/stdc++.h>
using namespace std;

int main()
{
	int k ; cin>>k;
	string s;  cin>>s;
	map<char,int> m;
	int a[105]={0};
	int n1= s.size(); int j = 0;
	for ( int i =0 ; i<n1; i++)
	{
		if (m[s[i]])
		continue;
		m[s[i]] = 1;
		a[j++] = i;
	}
	if (j<k)
	{
		cout<<"NO"<<endl;
		return 0;
	}
	cout<<"YES"<<endl;
	for (int j=0; j<k-1; j++)
	{
		for (int i = a[j]; i<a[j+1]; i++)
		{
			cout<<s[i];
		}
		cout<<endl;
	}
	for (int i = a[k-1]; i<n1; i++)
	cout<<s[i];
	cout<<endl;
}

